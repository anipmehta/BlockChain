<h2>Blocks</h2>
<h4> Total Users: <font color="blue"><%= Enum.count(@list) %> </font></h4>
<table class="container">
    <thead>
    <tr>
      <th>Block_Hash</th>
      <th>Action</th>
    </tr>
  </thead>
  <tbody>
  <%= for hash <- @list do %>
    <tr>
        <td><%= hash %></td>
        <td><a href="/block/<%= hash %>"<button type="button">View</button></td>
        </tr>
    <% end %>
  </tbody>
</table>
